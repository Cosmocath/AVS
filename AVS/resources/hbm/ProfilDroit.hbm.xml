<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-configuration PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="persistance.droit.beanDo.ProfilDroitDo"
		table="profildroit">
		<id name="idProfilDroit" column="id_ProfilDroit">
			<generator class="identity"></generator>
		</id>
		<many-to-one name="droitDo" column="id_Droit"></many-to-one>
		<many-to-one name="profilDo" column="id_Profil"
			lazy="false"></many-to-one>

	</class>

</hibernate-mapping>