<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd" >

<hibernate-mapping>
	<class name="persistance.commande.beanDo.CommandeProduitDo"
		table="commandeproduit">
		<id name="idCommandeProduit" column="id_CommandeProduit"
			type="integer">
			<generator class="identity"></generator>
		</id>
		<many-to-one name="commandeDo" column="idCommande"></many-to-one>
		<many-to-one name="produitVenduDo"
			column="id_ProduitVendu" lazy="false"></many-to-one>
		<property name="quantite" column="quantite" type="integer"></property>
	</class>
</hibernate-mapping>